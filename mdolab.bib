    booktitle   = {2006 12th International Power Electronics and Motion Control Conference},
    doi         = {10.1109/EPEPEMC.2006.4778548},
    keywords    = {machine testing;machine theory;permanent magnet machines;synchronous machines;design optimization;axial flux PM synchronous machine;DIRECT method;GA method;deterministic algorithm;stochastic algorithm;10 kW;Design optimization;Synchronous machines;Stochastic processes;Convergence;Algorithm design and analysis;Constraint optimization;Permanent magnet machines;Genetic algorithms;Testing;Quadratic programming},
