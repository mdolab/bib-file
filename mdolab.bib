@Article{Zymnis2007,
    author      = {Zymnis, Argyrios and Trichakis, Nikolaos and Boyd, Stephen and O'Neill, Dan},
    title       = {An Interior-Point Method for Large Scale Network Utility Maximization},
    year        = {2007},
    abstract    = {We describe a specialized truncated-Newton primal-dual interior-point method that solves large scale network utilitymaximization problems, with concave utility functions, efficiently and reliably. Our method is not decentralized, but easily scales to problems with 106 flows and links. We compare our method to a standard decentralized algo- rithm based on dual decomposition, and show by example that our method converges significantly faster for problems with congested networks or long routes. We describe an extension to problems that take into account delay or latency in the objective.}
}
