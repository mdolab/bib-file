@Article{Kroll2016,
    author      = {Kroll, N. and Abu-Zurayk, M. and Dimitrov, D. and Franz, T. and F{\"u}hrer, T. and Gerhold, T. and G{\"o}rtz, S. and Heinrich, R. and Ilic, C. and Jepsen, J. and J{\"a}gersk{\"u}pper, J. and Kruse, M. and Krumbein, A. and Langer, S. and Liu, D. and Liepelt, R. and Reimer, L. and Ritter, M. and Schw{\"o}ppe, A. and Scherer, J. and Spiering, F. and Thormann, R. and Togiti, V. and Vollmer, D. and Wendisch, J.-H.},
    title       = {DLR project Digital-X: towards virtual aircraft design and flight testing based on high-fidelity methods},
    doi         = {10.1007/s13272-015-0179-7},
    issn        = {1869-5590},
    journal     = {CEAS Aeronautical Journal},
    month       = {Mar},
    number      = {1},
    pages       = {3--27},
    url         = {https://doi.org/10.1007/s13272-015-0179-7},
    volume      = {7},
    year        = {2016},
    abstract    = {Numerical simulation is already an important cornerstone for aircraft design, although the application of highly accurate methods is mainly limited to the design point. To meet future technical, economic and social challenges in aviation, it is essential to simulate a real aircraft at an early stage, including all multidisciplinary interactions covering the entire flight envelope, and to have the ability to provide data with guaranteed accuracy required for development and certification. However, despite the considerable progress made there are still significant obstacles to be overcome in the development of numerical methods, physical modeling, and the integration of different aircraft disciplines for multidisciplinary analysis and optimization of realistic aircraft configurations. At DLR, these challenges are being addressed in the framework of the multidisciplinary project Digital-X (4/2012--12/2015). This paper provides an overview of the project objectives and
                  presents first results on enhanced disciplinary methods in aerodynamics and structural analysis, the development of efficient reduced order methods for load analysis, the development of a multidisciplinary optimization process based on a multi-level/variable-fidelity approach, as well as the development and application of multidisciplinary methods for the analysis of maneuver loads.}
}

