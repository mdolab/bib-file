@Article{Abreu2012,
    author      = {Rafael Abreu and Daniel Stich and Jos{\~A}{\copyright} Morales},
    title       = {On the generalization of the Complex Step Method},
    doi         = {10.1016/j.cam.2012.10.001},
    issn        = {0377-0427},
    journal     = {Journal of Computational and Applied Mathematics},
    keywords    = {sensitivities, complex step},
    number      = {0},
    url         = {http://www.sciencedirect.com/science/article/pii/S0377042712004207?v=s5},
    year        = {2012},
    abstract    = {We generalize the well known Complex Step Method for computing derivatives by introducing a complex step in a strict sense. Exploring different combinations of terms, we derive 52 approximations for computing the first order derivatives and 43 for the second order derivatives. For an appropriate combination of terms and appropriate choice of the step size in real and imaginary direction, 4 t h order accuracy can be achieved in a very simple and efficient scheme on a compact stencil. New different ways of computing second order derivatives in one single step are shown. Many of the first order derivative approximations avoid the problem of subtractive cancellation inherent to the classic finite difference approximations for real valued steps, and the superior accuracy and stability of the generalized Complex Step approximations is demonstrated for an analytic test function.}
}

