@Article{Siah_Sasena2004,
    author      = {{Eng Swee Siah} and M. {Sasena} and J. L. {Volakis} and P. Y. {Papalambros} and R. W. {Wiese}},
    title       = {Fast parameter optimization of large-scale electromagnetic objects using DIRECT with Kriging metamodeling},
    doi         = {10.1109/TMTT.2003.820891},
    issn        = {0018-9480},
    journal     = {IEEE Transactions on Microwave Theory and Techniques},
    keywords    = {computational electromagnetics;statistical analysis;electromagnetic interference;electromagnetic compatibility;mathematical programming;interpolation;finite element analysis;boundary integral equations;method of moments;convergence of numerical methods;electromagnetic coupling;frequency selective surfaces;slot antenna arrays;antenna theory;large-scale electromagnetic objects;fast parameter optimization;Kriging metamodeling;DIRECT algorithm;divided rectangles global-optimization;EM design;fast convergence bounds;shape optimization;slot-array frequency-selective surface;antenna location optimization;EM coupling;multisensor optimization;RF coupling constraints;vehicular chassis;wire harness;adaptive hybrid optimizer;superEGO;multilevel fast multipole moment method;electromagnetic interference;electromagnetic compatibility;finite element boundary integral;interpolation metamodeling;Large-scale systems;Metamodeling;Constraint optimization;Optimization methods;Slot
                  antennas;Radio frequency;Design optimization;Genetic algorithms;Neural networks;Shape},
    month       = {Jan},
    number      = {1},
    pages       = {276-285},
    volume      = {52},
    year        = {2004},
    abstract    = {With the advent of fast methods to significantly speed up numerical computation of large-scale realistic electromagnetic (EM) structures, EM design and optimization is becoming increasingly attractive. In recent years, genetic algorithms, neural network and evolutionary optimization methods have become increasingly popular for EM optimization. However, these methods are usually associated with a slow convergence bound and, furthermore, may not yield a deterministic optimal solution. In this paper, a new hybrid method using Kriging metamodeling in conjunction with the divided rectangles (DIRECT) global-optimization algorithm is used to yield a globally optimal solution efficiently. The latter yields a deterministic answer with fast convergence bounds and inherits both local and global-optimization properties. Three examples are given to illustrate the applicability of the method, Le., shape optimization for a slot-array frequency-selective surface, antenna location
                  optimization to minimize EM coupling from the antenna to RF devices in automobile structures, and multisensor optimization to satisfy RF coupling constraints on a vehicular chassis in the presence of a wire harness. In the first example, DIRECT with Kriging surrogate modeling was employed. In the latter two examples, the adaptive hybrid optimizer, superEGO, was used. In all three examples, emphasis is placed on the speed of convergence, as well as on the flexibility of the optimization algorithms.}
}

