@Article{Liu2017,
    author      = {Liu, Haitao and Xu, Shengli and Chen, Xudong and Wang, Xiaofang and Ma, Qingchao},
    title       = {Constrained global optimization via a {DIRECT}-type constraint-handling technique and an adaptive metamodeling strategy},
    doi         = {10.1007/s00158-016-1482-6},
    issn        = {1615-1488},
    journal     = {Structural and Multidisciplinary Optimization},
    month       = {Jan},
    number      = {1},
    pages       = {155--177},
    url         = {https://doi.org/10.1007/s00158-016-1482-6},
    volume      = {55},
    year        = {2017},
    abstract    = {Recent metamodel-based global optimization algorithms are very promising for box-constrained expensive optimization problems. However, few of them can tackle constrained optimization problems. This article presents an improved constrained optimization algorithm, called eDIRECT-C, for expensive constrained optimization problems. In the eDIRECT-C algorithm, we present a novel DIRECT-type constraint-handling technique that separately handles feasible and infeasible cells. This technique has no user-defined parameter and is beneficial for exploring the undetected feasible regions and boundary of feasible regions. We also employ an adaptive metamodeling strategy to build appropriate metamodel types for objective and constraints respectively. This strategy yields more accurate predictions and therefore significantly speeds up the convergence. To assess the performance of eDIRECT-C, we compare it with some state-of-the-art metamodel-based constrained optimization algorithms
                  and the original DIRECT algorithm on 13 benchmark problems and 4 engineering examples. The comparative results imply that the proposed algorithm is very promising for constrained problems in terms of the convergence speed, quality of final solutions and success rate.}
}

