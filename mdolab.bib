@Article{Brezillon2012a,
    author      = {Brezillon, Jo{\"{e}}l and Dwight, Richard P.},
    title       = {Applications of a discrete viscous adjoint method for aerodynamic shape optimisation of {3D} configurations},
    doi         = {10.1007/s13272-011-0038-0},
    isbn        = {1327201100},
    issn        = {1869-5582},
