@InProceedings{Widhalm2010,
    author      = {Widhalm, Markus and Brezillon, Jo\"{e}l and Ilic, Caslav and Leicht, Tobias},
    title       = {Investigation on Adjoint Based Gradient Computations for Realistic {3D} Aero-Optimization},
    address     = {Fort Worth, TX},
    booktitle   = {13th AIAA/ISSMO Multidisciplinary Analysis and Optimization Conference},
    number      = {September},
    pages       = {1--17},
    year        = {2010},
    abstract    = {A discrete adjoint method for efficiently computing gradients for aerodynamic shape op- timizations is presented. The chain itself involves an unstructuredmesh Reynolds-Averaged Navier-Stokes solver, and is suitable for the optimization of complex geometries in three dimensions. In addition to the discrete flow adjoint the method introduces a second ad- joint equation for the mesh deformation. Using the adjoint chain it is possible to evaluate the gradients of a cost function for the cost of one adjoint flow solution and one adjoint volume mesh deformation, without performing any (forward) mesh deformation. By choos- ing a suitable mesh deformation operator, like linear elasticity, the chain may be readily constructed by hand. Furthermore, this adjoint chain can be subsequently used with pa- rameterized surface grids. The accuracy and the computational savings of the resulting procedure is examined for the gradient-based shape optimization of a wing in inviscid
                  flow.}
}

