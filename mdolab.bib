@Article{Wilhelmsen1978,
    author      = {D. R. Wilhelmsen},
    title       = {Optimal quadrature for periodic analytic functions},
    doi         = {10.1137/0715020},
    issn        = {00361429},
    journal     = {SIAM Journal on Numerical Analysis},
    number      = {2},
    pages       = {291--296},
    publisher   = {Society for Industrial and Applied Mathematics},
    volume      = {15},
    year        = {1978},
    abstract    = {The trapezoidal rule is compared with other quadrature formulas of the general Hermite type which are interpolatory for the trigonometric functions. This comparison is done by describing integration on [ 0, 2π p] in terms of closed contour integration in the complex plane. Certain derivative-free error bounds are constructed and are shown to be optimal for the trapezoidal rule under mild regularity conditions on the integrand.}
}

@Article{Wilke2010,
    author      = {Wilke, Daniel N and Kok, Schalk and Groenwold, Albert A},
    title       = {The application of gradient-only optimization methods for problems discretized using non-constant methods},
    doi         = {10.1007/s00158-009-0389-x},
    isbn        = {0015800903},
    journal     = {Structural and Multidisciplinary Optimization},
    pages       = {433--451},
    volume      = {40},
    year        = {2010},
    abstract    = {We study the minimization of objective func- tions containing non-physical jump discontinuities. These discontinuities arise when (partial) differential equations are discretized using non-constant methods and the resulting numerical solutions are used in com- puting the objective function. Although the functions may become discontinuous, gradient information may be computed at every point. Gradient information is computable everywhere since every point has an associ- ated discretization for which (semi) analytical sensitiv- ities can be calculated. Rather than the construction of global approximations using only function value infor- mation to overcome the discontinuities, we propose to use only the gradient information. We elaborate on the modifications of classical gradient based optimization algorithms for use in gradient-only approaches, and we then present gradient-only optimization strategies using both BFGS and a new spherical quadratic ap- proximation for
                  sequential approximate optimization (SAO). We then use the BFGS and SAO algorithms to solve three problems of practical interest, both un- constrained and constrained.}
}

