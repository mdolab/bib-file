@Article{Hoff2010a,
    author      = {Arild Hoff and Henrik Andersson and Marielle Christiansen and Geir Hasle and Arne L{\o}kketangen},
    title       = {Industrial Aspects and Literature Survey: Fleet Composition and Routing},
    doi         = {10.1016/j.cor.2010.03.015},
    journal     = {Computers {\&} Operations Research},
    month       = {December},
    number      = {12},
    pages       = {2041--2061},
    volume      = {37},
    year        = {2010},
    abstract    = {The purpose of this paper is to describe industrial aspects of combined fleet composition and routing in maritime and road-based transportation, and to present the current status of research in the form of a comprehensive literature review. First, presents a classification of problems, and then focuses on a basic definition of combined fleet composition and routing: the fleet size and mix vehicle routing problem. A basic mathematical formulation from the literature is presented. Further, the literature of extended and related problems is described and categorized. Surveys of application oriented research in road-based and maritime transportation conclude the review. Finally, we contrast the literature with aspects of industrial applications from a critical, but constructive stance. Major issues for future work are suggested.}
}

