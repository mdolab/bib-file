@Article{Du2013a,
    author      = {Wenbo Du and Nansi Xue and Amit Gupta and Ann M. Sastry and Joaquim R. R. A. Martins and Wei Shyy},
    title       = {Optimization of {LiMn2O4} Electrode Properties in a Gradient- and Surrogate-Based Framework},
    doi         = {10.1007/s10409-013-0039-x},
    journal     = {Acta Mechanica Sinica},
    month       = {June},
